#include <iostream>

using namespace std;

int function(int n);

int main()
{
	int n;
	cin >> n;
	int *arr = new int[n * n];

	for (int i = 0; i < n * n; i++)
		for (int j = 0; j < n * n; j++)
		{
			arr[i] = j + 1;
			cout << "Value of " << i+1 << " element is " << arr[i] << endl;
		}
	delete[] arr;

	function(n);



}


int function(const int n)
{
	int var = n*n;
	//cout << var;
	return 1;
}
