#include <iostream>

using namespace std;

int main()
{
	int a = 0;
	int b = 0;
	int counter = 1;
	int max = 0;
	int i = 2;

	cin >> a >> b;

	if (a >= b)
	{
		max = a;
	}
	else
	{
		max = b;
	}

	while (a != 1 || b != 1 || i != max)
	{
		if (a % i == 0 && b % i == 0)
		{
			counter *= i;
			a = a / i;
			b = b / i;
		}
		else if ( b % i == 0)
		{
			counter *= i;
			b = b / i;
		}
		else if (a % i == 0 )
		{
			counter *= i;
			a = a / i;
		}
		i += 1;
	}
	cout << counter << endl;

}
