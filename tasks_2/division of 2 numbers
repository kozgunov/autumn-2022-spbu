#include <iostream>

using namespace std;

int main()
{
	int n = 0;
	int m = 0;
	int comparison_1 = 0;
	int comparison_2 = 0;
	int answer = 0;

	cin >> n; // 100
	cin >> m; // 2

	comparison_1 = m % n; // 2 = 2 % 100
	comparison_2 = n % m; // 0 = 100 % 2
	answer = comparison_2 / n + comparison_1 / m; // 1 = 0 / 100 + 2 / 2 

	cout << answer << endl;
}
