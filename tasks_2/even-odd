#include <iostream>

using namespace std;

int main()
{
	int a = 0;
	int b = 0;
	int c = 0;

	cin >> a;
	cin >> b;
	cin >> c;

	if (a % 2 == 0 || b % 2 == 0 || c % 2 == 0)
	{
		cout << "there is at least 1 even number" << endl;
	}
	if (a % 2 == 1 || b % 2 == 1 || c % 2 == 1)
	{
		cout << "there is at least 1 odd number" << endl;
	}

}
